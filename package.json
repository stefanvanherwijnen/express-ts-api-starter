{
  "name": "express-ts-api-starter",
  "version": "1.0.0",
  "description": "API Starter kit for Express in TypeScript with Paseto authentication",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "babel src --out-dir dist --extensions '.js,.jsx,.ts,.tsx' --copy-files",
    "dev": "nodemon src/server.ts --exec babel-node --presets @babel/env,@babel/typescript --extensions '.js,.jsx,.ts,.tsx' --config .nodemonrc.json | pino-pretty",
    "dev:debug": "nodemon src/server.ts --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "lint": "eslint --ext=.ts,.js src/",
    "lint:fix": "eslint --fix --ext=.ts,.js src/",
    "check:types": "tsc --noEmit",
    "test": "jest --verbose=true --runInBand",
    "configure": "babel-node --presets @babel/env,@babel/typescript --extensions '.js,.jsx,.ts,.tsx' console/configure.js"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "commander": "^4.1.0",
    "cors": "^2.8.5",
    "db-errors": "^0.2.3",
    "dotenv": "^8.2.0",
    "email-templates": "^7.0.2",
    "envfile": "^4.5.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "i18n": "^0.8.5",
    "is-docker": "^2.0.0",
    "is-email": "^1.0.0",
    "json-api-serializer": "^2.2.2",
    "knex": "^0.20.8",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.2",
    "objection": "^2.1.2",
    "paseto": "^1.0.5",
    "pg": "^7.18.1",
    "pino": "^5.16.0",
    "pug": "^3.0.1",
    "randomstring": "^1.1.5",
    "sqlite3": "^4.1.1",
    "superstruct": "^0.8.3",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.1",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.0",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0",
    "jest-environment-node": "^25.1.0",
    "nodemon": "^2.0.2",
    "pino-pretty": "^3.5.0",
    "supertest": "^4.0.2",
    "typescript": "^3.7.5"
  }
}